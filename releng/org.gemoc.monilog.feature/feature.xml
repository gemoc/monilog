      version="1.0.0.qualifier">

   <description>
      Xtext editor and standard library for MoniLog.
   </description>

   <requires>
      <import plugin="org.gemoc.monilog"/>
      <import plugin="org.gemoc.monilog.ide"/>
      <import plugin="org.eclipse.xtext.ui"/>
      <import plugin="org.eclipse.xtext.ui.shared"/>
      <import plugin="org.eclipse.xtext.ui.codetemplates.ui"/>
      <import plugin="org.eclipse.ui.editors" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.compare"/>
      <import plugin="org.eclipse.xtext.builder"/>
      <import plugin="org.eclipse.xtext.xbase.lib" version="2.14.0" match="greaterOrEqual"/>
      <import plugin="org.apache.log4j"/>
      <import plugin="org.eclipse.xtext.ide"/>
      <import plugin="org.eclipse.xtext.xbase.ide"/>
      <import plugin="org.eclipse.xtext"/>
      <import plugin="org.eclipse.xtext.xbase"/>
      <import plugin="org.eclipse.equinox.common" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.xtext.util"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.eclipse.core.runtime" version="3.17.100" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtend.lib" version="2.14.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.equinox.p2.engine"/>
   </requires>

   <plugin
         id="org.gemoc.monilog.ui"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.gemoc.monilog.ide"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.gemoc.monilog"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"/>

   <plugin
         id="org.gemoc.instrument.interface"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"
         unpack="false"/>

</feature>
